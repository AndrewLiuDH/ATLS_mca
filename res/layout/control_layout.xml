        <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="20.0sp"
	        android:padding="16dp"
	        android:text="执行器2：" 
        	/>
        
        <com.digihuman.atls_mca.ui.SwitchButton
            android:id="@+id/switch2_onoff_switcher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            />
        
